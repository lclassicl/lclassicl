$(document).ready(function() {
	$(window).scroll(function() {
        if ($(document).scrollTop() < 120)
		{
			if ($('#gnb').hasClass('fixed'))
            {
				$('#gnb').removeClass('fixed');
                $('#wrapper').css('padding-top', 0);
                $('#new_menu').fadeIn();
            }
            if ($('#topUp:visible'))
                $('#topUp').fadeOut();
		}
		else
		{
            if (!$('#gnb').hasClass('fixed'))
            {
			    $('#gnb').addClass('fixed');
                $('#wrapper').css('padding-top', $('#gnb').outerHeight()) + 'px';
                $('#new_menu').fadeOut();
            }
            if ($('#topUp:hidden'))
